     <parameter name="environment" value="chrome"></parameter>
        <classes>
            <class name="automationTesting.runner.AutomationTestRunner"/>
        </classes>
    </test>
    <test name="AutomationFirefox">
        <parameter name="environment" value="firefox"></parameter>
        <classes>
            <class name="automationTesting.runner.AutomationTestRunner"/>
        </classes>
    </test>
</suite>
